{
  "manifest_version": 2,
  "name": "LFB+",
  "version": "2.5",
  "author": "Harald Hentschel",
  "description": "Improves the presentation of the pages for searching and the presentation of a training.",
  "homepage_url": "https://github.com/Mr-DblH/LFBPlus",


  "icons": {
    "48": "icons/lfbPlusIcon-48.png",
    "96": "icons/lfbPlusIcon-96.png",
    "128": "icons/lfbPlusIcon-128.png"
  },

  "permissions": [
    "clipboardWrite"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.kultus-bw.de/*"],
      "js": ["lfbPlus.js"]
    }
  ]
}

